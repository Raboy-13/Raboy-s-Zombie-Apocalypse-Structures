{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.illager_settlement": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"spawn": "true"
						}
					]
				},
				"spawn": {
					"on_entry": [
						"/function illager_settlement/illager_settlement"
					],
					"transitions": [
						{
							"despawn": "true"
						}
					]
				},
				"despawn": {
					"on_entry": [
						"/event entity @s rza:despawn"
					]
				}
			}
		}
	}
}